le(50, (i + 1) * 25))\n}\n\nconst start = () => {\n  ctx.clearRect(0, 0, W, H)\n  circles.forEach((c) => {\n    c.render()\n    c.move()\n    c.checkHit()\n  })\n\n  window.requestAnimationFrame(start)\n}\n\n// setInterval(() => {\n//   start()\n// }, 1000)\n\nstart()\n"},"lineCount":null}},"error":null,"hash":"e47c6429dce6705c146ad972b12fc145","cacheData":{"env":{}}}